<div>
  <div class='alerts' id='id06' style='display: block;'>
    <h1>{{resData.message}} </h1>
    <span
      onclick="document.getElementById('id06').style.display='none'"
      class='w3-button w3-large w3-display-topright'
      style='border-top-right-radius: 10px;'
    >&times;</span>
  </div>

  <div class='container'>
    <input type='checkbox' name='' id='check' />

    <div class='logo-container'>
      <img src='/assets/images/logo2.png' alt='' srcset='' />
    </div>

    <div class='nav-btn'>
      <div class='nav-links'>
        <ul>
          {{! <li class="nav-link" style="--i: 0.6s">
                <a href="#">Search Design</a>
              </li> }}
          {{! <li class="nav-link" style="--i: 0.85s">
                <a href="#">Contractor List<i></i></a>
                    <li class="dropdown-link">
                    </li>
              </li> }}
          {{! <li class="nav-link" style="--i: 1.35s">
                <a href="#">About us</a>
              </li> }}
        </ul>
      </div>

      <div class='log-sign' style='--i: 1.8s'>
        <a href='' class='btn-logout'>Log out</a>
      </div>
    </div>

    <div class='hamburger-menu-container'>
      <div class='hamburger-menu'>
        <div></div>
      </div>
    </div>
  </div>
  <div>
    <form action=''>
      <div class='profile'>
        <img src='/assets/images/undraw_male_avatar_323b (1).svg' alt='' />
      </div>
      <div class='info'>
        <h3>{{{Fullname}}}</h3>
        <h3>{{{Address}}}</h3>
        <h3>{{{EmailAddress}}}</h3>

      </div>
      <div class='button'>
        <input
          type='button'
          value='Edit Profile'
          onClick="window.location.href='http://localhost:8082/cprofile'"
          class='btnedit'
        />
        <input
          type='button'
          value='Messages'
          onClick="window.location.href='http://localhost:8082/messages'"
          class='btnedit'
        />

      </div>
    </form>

  </div>

  <hr />
  {{! <img
    src='/assets/houses/1642272373547--asdasd - Copy.jpg'
    alt='Mountains'
    class='i'
    style='width:100%'
  /> }}
  <div class='main' id='myportfolio'>
    <h2 style="font-family: 'Paytone One', sans-serif; color: white;">MY TOP
      PROJECTS</h2>

    <div class='addproject'>
      <input
        onclick="document.getElementById('id01').style.display='block'"
        type='button'
        value='Add PROJECT'
        class='btnedit'
      />
    </div>
    <div class='btn-pick' id='myBtnContainer'>
      <button class='btn active' onclick="filterSelection('all')">
        Show all</button>
      <button class='btn' onclick="filterSelection('nature')">
        Bungalow Type</button>
      <button class='btn' onclick="filterSelection('cars')">
        Two Storey Type</button>
      <button class='btn' onclick="filterSelection('people')">
        Modern Type</button>
    </div>

    <!-- Portfolio Gallery Grid -->
    <div class='row'>
      {{#each Houses}}
        <div class='column nature'>
          <div class='content'>
            <img src='{{{frontView}}}' alt='' class='i' style='width:100%' />
            <form onsubmit='return ViewDetails(event)'>
              <div class='edit'>
                <input class='bt-edit' type='submit' value='View Details' />
                <input type='hidden' value='{{{_id}}}' name='id' />
                <input
                  onclick="document.getElementById('id03').style.display='block'"
                  class='bt-edit'
                  type='button'
                  value='Edit Details'
                />
              </div>
            </form>
            <form onsubmit='return Deletedetails(event)'>
              <div class='delete'>
                <input class='bt-edits' type='submit' value='Delete' />
                <input type='hidden' value='{{{_id}}}' name='id' />
              </div>
            </form>
          </div>
        </div>
      {{/each}}
    </div>

  </div>
  <div id='id01' class='w3-modal w3-animate-opacity' style='overflow: hidden;'>
    <div class='w3-modal-content w3-card-4' style='border-radius: 10px;'>
      <header
        class='w3-container'
        style='border-top-left-radius: 10px; border-top-right-radius: 10px;'
      >
        <span
          onclick="document.getElementById('id01').style.display='none'"
          class='w3-button w3-large w3-display-topright'
          style='border-top-right-radius: 10px;'
        >&times;</span>
        <h2
          style='color: white;  
          '
        >House Details</h2>
      </header>
      <div class='w3-container'>
        <div class='updatehouse'>
          <form
            id='UPLOAD'
            method='POST'
            action='/update'
            enctype='multipart/form-data'
            style='display: flex; justify-content: center; align-items: center; flex-direction: column;'
          >
            <h1>Upload at least 4 images:</h1>
            <input
              required
              type='file'
              id='files'
              class='files'
              name='imageHouse'
              multiple
            /><br /><br />
            <label for='price'>House Price</label>
            <input
              class='inputes'
              type='text'
              name='price'
              id='Price'
              required
            />
            <br />
            <label for='bedrooms'>House Type</label>
            <select class='type' name='Housetype' id='' required>
              <option value='Bungalow'>Bungalow</option>
              <option value='Two Story'>Two-Story</option>
              <option value='Modern'>Modern</option>
            </select>
            <br />
            <label for=''>LotSize (Sqm)</label>
            <input class='inputes' type='text' name='lotSize' required /><br />
            <label for=''>BedRoom</label>
            <input class='inputes' type='text' name='bedRoom' required /><br />
            <label for=''>RestRoom</label>
            <input class='inputes' type='text' name='cr' required /><br />
            <input
              type='submit'
              name='submit'
              class='btns'
              {{! onclick="document.getElementbyId('id06').style.display='block'" }}
            />
          </form>
        </div>
      </div>
      <footer class='w3-container'>
      </footer>
    </div>
  </div>
  <div id='id02' class='w3-modal w3-animate-opacity' style='overflow: hidden;'>
    <div class='w3-modal-content w3-card-4' style='border-radius: 10px;'>
      <header
        class='w3-container'
        style='border-top-left-radius: 10px; border-top-right-radius: 10px;'
      >
        <span
          onclick="document.getElementById('id02').style.display='none'"
          class='w3-button w3-large w3-display-topright'
          style='border-top-right-radius: 10px;'
        >&times;</span>
        <h4
          style="color: white; text-align:center; font-family: 'Paytone One', sans-serif;
            "
        >House Details</h4>
      </header>
      <div class='w3-container'>
        <div
          class='grid-container'
          style='   padding: 10px;
            display: grid; align-items: center; justify-content: center; grid-template-columns: auto auto;'
        >
          <div class='grid-item' style='margin: 10px;'>
            <img
              src='/assets/images/beautiful-bungalow-house-design-ideas-18.png'
              alt=''
              srcset=''
              id='front-image'
              style=' border-radius: 10px; margin-top: 2%; height: 20vh; width: 100%;'
            />
          </div>
          <div class='grid-item'>
            <img
              src='/assets/images/beautiful-bungalow-house-design-ideas-18.png'
              alt=''
              srcset=''
              id='side-image'
              style=' border-radius: 10px; margin-top: 2%; height: 20vh; width: 100%;'
            />
          </div>
          <div class='grid-item' style='margin: 10px;'>
            <img
              src='/assets/images/beautiful-bungalow-house-design-ideas-18.png'
              alt=''
              srcset=''
              id='rear-image'
              style=' border-radius: 10px; margin-top: 2%; height: 20vh; width: 100%;'
            />
          </div>
          <div class='grid-item'>
            <img
              src='/assets/images/beautiful-bungalow-house-design-ideas-18.png'
              alt=''
              srcset=''
              id='inside-image'
              style=' border-radius: 10px; margin-top: 2%; height: 20vh; width: 100%;'
            />
          </div>
        </div>
      </div>
      <div class='details'>
        <div class='dtls'>
          <h2>House Price: <h3 id='house-price'></h3></h2>
          <h2>House Type:<h3 id='house-type'></h3></h2>

        </div>
        <div class='send'>
          <h2>No. of Cr:<h3 id='cr'></h3></h2>
          <h2>BedRooms:<h3 id='bedrooms'></h3></h2>

        </div>
      </div>
      <div class='noobs'>
        <h2>Lot Size(sqm):<h3 id='lotsize'></h3></h2>
        {{! <input class='inters' type='button' value='Im Interested!' /> }}

      </div>
      <footer class='w3-container'>
      </footer>
    </div>
  </div>

  {{! <div id="id03" class="w3-modal w3-animate-opacity" style="overflow: hidden;">
      <div class="w3-modal-content w3-card-4" style="border-radius: 10px;">
        <header class="w3-container " style="border-top-left-radius: 10px; border-top-right-radius: 10px;"> 
          <span onclick="document.getElementById('id03').style.display='none'" 
          class="w3-button w3-large w3-display-topright" style="border-top-right-radius: 10px;">&times;</span>
          <h2 style="color: white;  
          ">House Details</h2>
        </header>
        <div class="w3-container">
          <div class="updatehouse">
            <form id="UPLOAD" method='PUT'  action='/cprofile' enctype="multipart/form-data" style="display: flex; justify-content: center; align-items: center; flex-direction: column;">
              <h1>Upload at least 4 images:</h1>
              <input type="file" id="files" class="files" name="images" multiple><br><br>
              <label for="price">House Price</label>
              <input  class="inputes" type="text" name="price" id="Price"> <br>
              <label for="bedrooms">House Type</label>
              <input class="inpute" type="text" name="housetype" id="houseType"><br>
                 <label for="bedrooms">BedRooms</label>
              <input class="inpute" type="text" name="bedrooms" id="bedrooms"><br>
                 <label for="bedrooms">Comfort Rooms</label>
              <input class="inpute" type="text" name="comfort" id="comfort"><br>
                 <label for="bedrooms">Lot Size</label>
              <input class="inpute" type="text" name="lotsize" id="lotsize"><br>
              <input type="submit" name="submit" class="btns">
