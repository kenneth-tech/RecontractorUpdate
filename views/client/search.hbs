    <div class='hamburger-menu-container'>
      <div class='hamburger-menu'>
        <div></div>
      </div>
    </div>
  </div>

</div>

<div class='main' id='myportfolio'>

  <h2 style="font-family: 'Paytone One', sans-serif; color: white;">House Design</h2>
  <div class='btn-pick' id='myBtnContainer'>
    <a href='http://localhost:8082/search'>Show All</a>
    <a href='http://localhost:8082/search/bungalow'>Bungalow Type</a>
    <a href='http://localhost:8082/search/twostorey'>Two Storey Type</a>
    <a href='http://localhost:8082/search/modern'>Modern Type</a>

  </div>

  <!-- Portfolio Gallery Grid -->
  <div class='row'>
    {{#each resData.data}}
      <div class='column nature'>
        <div class='content'>
          <img
            src='{{{frontView}}}'
            alt='Mountains'
            class='i'
            style='width:100%'
          />
          <form onsubmit='return SearchDetails(event)'>
            <div class='edit'>
              <input class='btdit' type='submit' value='View Details' />
              <input type='hidden' value='{{{_id}}}' name='id' />
            </div>
          </form>
        </div>
      </div>
    {{/each}}
  </div>

  <div id='id02' class='w3-modal w3-animate-opacity' style='overflow: hidden;'>
    <div class='w3-modal-content w3-card-4' style='border-radius: 10px;'>
      <header
        class='w3-container'
        style='border-top-left-radius: 10px; border-top-right-radius: 10px;'
      >
        <span
          onclick="document.getElementById('id02').style.display='none'"
          class='w3-button w3-large w3-display-topright'
          style='border-top-right-radius: 10px;'
        >&times;</span>
        <h4
          style="color: white; text-align:center;  font-family: 'Paytone One', sans-serif;
            "
        >House Details</h4>
      </header>
      <div class='w3-container'>
        <div
          class='grid-container'
          style='   padding: 10px;
            display: grid; align-items: center; justify-content: center; grid-template-columns: auto auto;'
        >
          <div class='grid-items' style='margin:;'>
            <img
              src='/assets/images/beautiful-bungalow-house-design-ideas-18.png'
              alt=''
              srcset=''
              id='frontimage'
              style=' border-radius: 10px; margin-top: 2%; height: 20vh; width: 100%;'
            />
          </div>
          <div class='grid-items'>
            <img
              src='/assets/images/beautiful-bungalow-house-design-ideas-18.png'
              alt=''
              srcset=''
              id='sideimage'
              style=' border-radius: 10px; margin-top: 2%; height: 20vh; width: 100%;'
            />
          </div>
          <div class='grid-items' style='margin: 10px;'>
            <img
              src='/assets/images/beautiful-bungalow-house-design-ideas-18.png'
              alt=''
              srcset=''
              id='rearimage'
              style=' border-radius: 10px; margin-top: 2%; height: 20vh; width: 100%;'
            />
          </div>
          <div class='grid-items'>
            <img
              src='/assets/images/beautiful-bungalow-house-design-ideas-18.png'
              alt=''
              srcset=''
              id='insideimage'
              style=' border-radius: 10px; margin-top: 2%; height: 20vh; width: 100%;'
            />
          </div>
        </div>
      </div>
      <div class='details'>
        <div class='dtls'>
          <h2>House Price:<h5 id='houseprice'></h5></h2>
          <h2>House Type:<h5 id='housetype'></h5></h2>
          <h2>BedRooms:<h5 id='Bedrooms'></h5></h2>
        </div>
        <div class='send'>
          <h2>Lot Size:<h5 id='Lotsize'></h5></h2>
          <h2>No. of Cr:<h5 id='CR'></h5></h2>
          <input class='inters' type='button' value='Im Interested!' />
        </div>
      </div>
      <footer class='w3-container'>
      </footer>
    </div>
  </div>
</div>