  <div class='container'>
    <input type='checkbox' name='' id='check' />

    <div class='logo-container'>
      <img src='/assets/images/logo2.png' alt='' srcset='' />
    </div>

    <div class='nav-btn'>
      <div class='nav-links'>
        <ul>
          <li class='nav-link' style='--i: 0.6s'>
            <a href='http://localhost:8082/search'>Search Design</a>
          </li>
          <li class='nav-link' style='--i: 0.85s'>
            <a href='http://localhost:8082/clist'>Contractor List<i></i></a>
            <li class='dropdown-link'>
            </li>
          </li>
          <li class='nav-link' style='--i: 1.35s'>
            <a href='#'>About us</a>
          </li>
        </ul>
      </div>

      <div class='log-sign' style='--i: 1.8s'>
        <a href='#' class=''></a>
      </div>
